<Page x:Class="Pages.Personal_Data"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:Pages"
      mc:Ignorable="d"
      Title="Personal_Data" Height="450" Width="800">
    <Grid>
        <TextBlock Margin="50" Text="Личные данные" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Top" />
        <TextBox Margin="100" Text="Введите имя" FontSize="20" VerticalAlignment="Top"/>
        <TextBox Margin="100 140" Text="Введите Фимилию" FontSize="20" VerticalAlignment="Top"/>
        <DatePicker FontSize="20" Margin="100 180" VerticalAlignment="Top" Height="30" />
    </Grid>
</Page>
